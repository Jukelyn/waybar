  "spacing": 4,
  "modules-left": [
    "custom/archicon",
    "hyprland/workspaces",
    "pulseaudio",
    "backlight",
    "network",
  ],
  "modules-center": [
    "power-profiles-daemon",
    "mpd"
  ],
  "modules-right": [
    "disk",
    "cpu",
    "memory",
    "temperature",
    "battery",
